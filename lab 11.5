#include <stdio.h>
#include <math.h>
int main()
{
	int x;
	scanf_s("%d", &x);
	if (x == 0)
		printf("zero number");
	else if (x < 0)
		if (x % 2 != 0)
			printf("negative odd number");
		else
			printf("negative even number");
	else
		if (x % 2 != 0)
			printf("an odd positive integer");
		else 
			printf("positive even number");
	return 0;
}
